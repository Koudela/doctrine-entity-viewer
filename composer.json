{
    "name": "doctine/entity-viewer",
    "description": "doctrine entity viewer",
    "type": "project",
    "license": "MIT",
    "version": "0.0-dev",
    "authors": [
        {
            "name": "Thomas Koudela",
            "email": "thomas@koudela.net"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
        "php": "^7.2 || ^8.0",
        "doctrine/orm": "^2.8",
        "symfony/var-dumper": "^5.2",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "doctrine\\EntityViewer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "doctrine\\EntityViewer\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    }
}
